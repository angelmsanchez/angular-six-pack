{
  "name": "angular-six-pack",
  "version": "1.0.0",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "build:dev": "ng build",
    "build:electron:macos": "electron-packager . --platform=darwin",
    "build:electron:windows": "electron-packager . --platform=win32",
    "build:prod": "ng build --prod",
    "build:prod:chunk": "ng build --prod --build-optimizer --vendor-chunk",
    "deploy": "yarn run build:prod && angular-cli-ghpages",
    "e2e": "ng e2e",
    "http-server": "http-server dist/ -c-1",
    "lint": "ng lint",
    "ng": "ng",
    "start": "ng serve --open",
    "start:prod": "rm -r dist && ng build --prod && http-server dist --cors -o",
    "server:electron": "ng build && yarn run electron .",
    "server:electron:prod": "ng build --prod && yarn run electron .",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.0",
    "@angular/common": "6.1.0",
    "@angular/compiler": "6.1.0",
    "@angular/core": "6.1.0",
    "@angular/forms": "6.1.0",
    "@angular/http": "6.1.0",
    "@angular/platform-browser": "6.1.0",
    "@angular/platform-browser-dynamic": "6.1.0",
    "@angular/pwa": "0.7.2",
    "@angular/router": "6.1.0",
    "@angular/service-worker": "6.1.0",
    "core-js": "2.5.7",
    "http-server": "0.11.1",
    "rxjs": "6.2.2",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "6.1.2",
    "@angular/compiler-cli": "6.1.0",
    "@angular/language-service": "6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular-cli-ghpages": "0.5.2",
    "codelyzer": "4.2.1",
    "electron": "2.0.6",
    "electron-packager": "^12.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngx-electron": "^1.0.4",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "5.11.0",
    "typescript": "2.7.2"
  }
}
